package bst;

public class BST_main {
	public static void main(String[] args) {
		BinarySearchTree<Integer> bst = new BinarySearchTree<Integer>();
		BSTVisualizer bstv = new BSTVisualizer("Tr√§d", 500, 500);
		bst.add(50);
		bst.add(40);
		bst.add(60);
		bst.add(40);
		bst.add(30);
		bst.add(55);
		bst.add(65);
		bst.add(66);
		bst.add(56);
		bst.add(58);
		bst.add(57);
		System.out.println("Size: " + bst.size());
		System.out.println("Height: " + bst.height());
		bst.printTree();
		bstv.drawTree(bst);
	}
}
